{
  "name": "starter-vite-react",
  "private": true,
  "version": "1.0.0",
  "description": "Opinionated Vite starter template with ESLint, Prettier, Stylelint and more.",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabgrel10/vite-react-starter"
  },
  "keywords": [
    "vite",
    "react",
    "starter",
    "template"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint:eslint": "eslint move --ext .ts,.tsx",
    "lint:stylelint": "stylelint **/*.{css,scss,jsx}",
    "fix:eslint": "eslint src --fix --ext .ts,.tsx",
    "fix:stylelint": "stylelint --fix  ./**/*.{css,scss}",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "fix": "npm run fix:eslint && npm run fix:stylelint",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "jotai": "^2.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "^6.3.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@namics/stylelint-bem": "^7.0.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "27.0.1",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^12.10.3",
    "jest": "^29.0.1",
    "jsdom": "^22.1.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.16",
    "postcss-scss": "^4.0.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "prettier-plugin-style-order": "^0.2.2",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.54.5",
    "stylelint": "^14.11.0",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "5.1.6",
    "vi-fetch": "^0.8.0",
    "vite": "^3.0.9",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.34.6",
    "webpack": "^5.74.0"
  }
}
